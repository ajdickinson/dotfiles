{
    "initial_working_directory": "~",
    "posix_terminal_shell": "custom",
    "editor_theme": "GitHub {rsthemes}",
    "editor_keybindings": "sublime",
    "panes": {
        "quadrants": [
            "Source",
            "TabSet1",
            "Console",
            "TabSet2"
        ],
        "tabSet1": [
            "Presentation"
        ],
        "tabSet2": [
            "Environment",
            "History",
            "Files",
            "Plots",
            "Packages",
            "Help",
            "Viewer",
            "Presentations"
        ],
        "hiddenTabSet": [
            "Connections",
            "Build",
            "VCS",
            "Tutorial"
        ],
        "console_left_on_top": false,
        "console_right_on_top": true,
        "additional_source_columns": 0
    },
    "rmd_viewer_type": "pane",
    "highlight_selected_line": true,
    "scroll_past_end_of_document": true,
    "syntax_color_console": true,
    "save_workspace": "never",
    "remove_history_duplicates": true,
    "full_project_path_in_window_title": true,
    "rmd_chunk_output_inline": false,
    "rmd_preferred_template_path": "/opt/homebrew/lib/R/4.1/site-library/learnr/rmarkdown/templates/tutorial",
    "jobs_tab_visibility": "shown",
    "default_project_location": "~/Documents/playground",
    "global_theme": "alternate",
    "custom_shell_command": "/opt/homebrew/bin/zsh",
    "terminal_bell_style": "none",
    "python_type": "conda",
    "python_version": "3.9.5",
    "python_path": "/opt/homebrew/Caskroom/miniconda/base/bin/python3.9",
    "soft_wrap_r_files": true,
    "source_with_echo": true,
    "insert_native_pipe_operator": true
}